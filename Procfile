web mkdir -p /tmp/files && java -javaagent:/app/newrelic/newrelic.jar -Dbet.ftpStoragePath=${FTP_STORAGE_PATH} -Dbet.ftpServer=${FTP_HOST} -Dbet.ftpUser=${FTP_USERNAME} -Dbet.ftpPassword=${FTP_PASSWORD} -Dbet.graphiteHost=${GRAPHITE_HOST} -Dbet.graphiteApiKey=${HOSTEDGRAPHITE_APIKEY} -Dserver.port=$PORT -Dbet.storagePath=/tmp/files $JAVA_OPTS -jar target/bet.war